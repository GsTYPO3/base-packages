{
    "name": "typo3/base-package-install-helper",
    "license": "GPL-2.0-or-later",
    "type": "project",
    "require": {
        "gilbertsoft/base-package-test": "@dev",
        "typo3/base-package-installer-plugin": "^0.1@dev"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.28",
        "typo3/coding-standards": "^0.5"
    },
    "repositories": {
        "installer": {
            "type": "path",
            "url": "installer"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "typo3/base-package-installer-plugin": true
        },
        "sort-packages": true
    }
}
